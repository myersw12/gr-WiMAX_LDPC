<?xml version="1.0"?>
<block>
  <name>ldpc_encoder</name>
  <key>wimaxldpc_ldpc_encoder</key>
  <category>[wimaxldpc]</category>
  <import>import wimaxldpc</import>
  <make>wimaxldpc.ldpc_encoder($rate, $z, $num_threads)</make>

  <param>
      <name>Code Rate</name>
      <key>rate</key>
      <type>int</type>
      <option>
          <name>Half Rate</name>
          <key>0</key>
      </option>
      <option>
          <name>Two Thirds A</name>
          <key>1</key>
      </option>
      <option>
          <name>Two Thirds B</name>
          <key>2</key>
      </option> 
      <option>
          <name>Three Quarters A</name>
          <key>3</key>
      </option> 
      <option>
          <name>Three Quarters B</name>
          <key>4</key>
      </option> 
      <option>
          <name>Five Sixths</name>
          <key>5</key>
      </option> 
  </param>

  <param>
      <name>Z Factor</name>
      <key>z</key>
      <value>96</value>
      <type>int</type>
  </param>
 
  <param>
      <name>Number of Threads</name>
      <key>num_threads</key>
      <type>int</type>
      <option>
          <name>1</name>
          <key>1</key>
      </option>
      <option>
          <name>2</name>
          <key>2</key>
      </option>
      <option>
          <name>4</name>
          <key>4</key>
      </option> 
      <option>
          <name>6</name>
          <key>6</key>
      </option> 
      <option>
          <name>8</name>
          <key>8</key>
      </option> 
      <option>
          <name>16</name>
          <key>16</key>
      </option> 
  </param>
  
  <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
  
</block>
